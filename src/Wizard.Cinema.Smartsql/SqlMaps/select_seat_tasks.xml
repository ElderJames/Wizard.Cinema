<?xml version="1.0" encoding="utf-8" ?>
<SmartSqlMap Scope="SelectSeatTask"  xmlns="http://SmartSql.net/schemas/SmartSqlMap.xsd"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://SmartSql.net/schemas/SmartSqlMapConfig.xsd Smartsql/SmartSqlMap.xsd">

  <ParameterMaps>
    <ParameterMap Id="SessionParamMap">
      <Parameter Property="SeatNos" TypeHandler="Json" />
    </ParameterMap>
  </ParameterMaps>

  <ResultMaps>
    <ResultMap Id="QuerySessionResultMap">
      <Result Column="SeatNos" Property="SeatNos" TypeHandler="Json" />
    </ResultMap>
  </ResultMaps>

  <Statements>
    <Statement Id="BatchInsert" ParameterMap="SessionParamMap">
      INSERT INTO select_seat_tasks
      (TaskId
      ,SessionId
      ,WizardId
      ,WechatName
      ,SerialNo
      ,Total
      ,Status
      ,SeatNos
      ,BeginTime
      ,EndTime
      ,CreateTime
      )VALUES
      <For Property="tasks" Key="T" Separator="," Open="" Close="" Prepend="">
        (@TaskId
        ,@SessionId
        ,@WizardId
        ,@WechatName
        ,@SerialNo
        ,@Total
        ,@Status
        ,@SeatNos
        ,@BeginTime
        ,@EndTime
        ,@CreateTime)
      </For>
    </Statement>
  </Statements>
</SmartSqlMap>
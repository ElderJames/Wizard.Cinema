<?xml version="1.0" encoding="utf-8" ?>
<SmartSqlMap Scope="Cinema"  xmlns="http://SmartSql.net/schemas/SmartSqlMap.xsd"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://SmartSql.net/schemas/SmartSqlMapConfig.xsd ../smartsql/SmartSqlMap.xsd">

  <Statements>
    <Statement Id="Insert">
      INSERT INTO city_cinema
      (CinemaId
      ,Name
      ,CityId
      ,Address
      ,LastUpdateTime)
      VALUES
      (@CinemaId
      ,@Name
      ,@CityId
      ,@Address
      ,@LastUpdateTime
      );
      SELECT last_insert_rowid();
    </Statement>

    <Statement Id="InsertBatch">
      INSERT INTO city_cinema
      (CinemaId
      ,Name
      ,CityId
      ,Address
      ,LastUpdateTime)
      VALUES
      <For Prepend="" Property="cinemas" Key="T" Open="" Separator="," Close="">
        (@CinemaId
        ,@Name
        ,@CityId
        ,@Address
        ,@LastUpdateTime
        )
      </For>
    </Statement>
  </Statements>
</SmartSqlMap>